{
  "name": "react-components",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.0-alpha.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.16",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^3.1.0",
    "antd": "^5.3.2",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "cypress": "^12.10.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "history": "^5.3.0",
    "moment": "^2.29.4",
    "notiflix": "^3.2.6",
    "prettier-eslint": "^15.0.1",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-loader-spinner": "^5.3.4",
    "react-redux": "^8.0.5",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "rtk-query": "^0.0.0",
    "sinon": "^15.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "vite": "^4.2.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-plugin-ts": "^1.3.2-1",
    "vitest": "^0.29.8",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "nodemon",
    "cypress": "cypress open",
    "run": "cypress run --env coverage=false",
    "cover": "cypress run --env coverage=true",
    "build": "tsc && vite build",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/main.tsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint-fix": "eslint --fix rsc",
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "vitest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@types/sinon": "^10.0.13",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitest/coverage-c8": "^0.29.8",
    "eslint": "^8.14.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "fetch-mock-jest": "^1.5.1",
    "gh-pages": "^5.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite-plugin-react": "^4.0.1"
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  }
}
